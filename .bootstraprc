---
# You can use comments here
bootstrapVersion: 4

env:
  development:
    extractStyles: false
  production:
    extractStyles: true

preBootstrapCustomizations: ./js/styles/bootstrap-pre-customizations.scss

useFlexbox: true

styleLoaders:
  - style
  - css
  - sass

styles:
  print: true

scripts:
  alert: true
  button: true
